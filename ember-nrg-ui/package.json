    "ember-resize-observer-service": "^0.4.5",
    "ember-responsive": "4.0.2",
    "ember-tether": "^2.0.1",
    "ember-truth-helpers": "3.0.0",
    "ember-validators": "4.0.0",
    "fomantic-ui-css": "2.9.2",
    "git-repo-info": "^2.1.1",
    "sass": "1.43.5",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@babel/plugin-proposal-decorators": "^7.22.6",
