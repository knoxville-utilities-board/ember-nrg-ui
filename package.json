{
  "name": "ember-nrg-ui",
  "version": "4.1.12",
  "description": "Opinionated UI addon based on how KUB scaffolds web applications",
  "keywords": [
    "ember-addon",
    "Fomantic UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/knoxville-utilities-board/ember-nrg-ui"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build-dummy-app": "ember build --environment production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:cicd": "ember test -r xunit",
    "test:ember-compatibility": "ember try:each",
    "format": "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write ."
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.4",
    "dayjs": "1.11.2",
    "ember-auto-import": "^2.4.3",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-flash": "4.0.0",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-cli-sass": "10.0.1",
    "ember-concurrency": "2.2.0",
    "ember-concurrency-decorators": "2.0.3",
    "ember-get-config": "0.5.0",
    "ember-is-mobile": "4.1.2",
    "ember-keyboard": "8.2.0",
    "ember-modifier": "3.2.7",
    "ember-resize-observer-service": "^0.4.5",
    "ember-responsive": "4.0.2",
    "ember-tether": "^2.0.1",
    "ember-truth-helpers": "3.0.0",
    "ember-validators": "4.0.0",
    "fomantic-ui-css": "2.9.2",
    "git-repo-info": "^2.1.1",
    "markdown-it": "12.3.2",
    "sass": "1.43.5",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.8.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "4.4.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-freestyle": "^0.17.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.16.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.2",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://knoxville-utilities-board.github.io/ember-nrg-ui/"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "clean-css": "^4.1.11",
    "glob-parent": "^5.1.2"
  }
}
