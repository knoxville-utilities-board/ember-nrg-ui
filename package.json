{
  "name": "ember-nrg-ui",
  "version": "4.7.8",
  "description": "Opinionated UI addon based on how KUB scaffolds web applications",
  "keywords": [
    "ember-addon",
    "Fomantic UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/knoxville-utilities-board/ember-nrg-ui"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build-dummy-app": "ember build --environment production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:cicd": "ember test -r xunit",
    "test:ember-compatibility": "ember try:each",
    "format": "prettier --config ./.prettierrc.js --ignore-path ./.prettierignore --write .",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
