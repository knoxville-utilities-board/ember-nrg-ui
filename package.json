{
  "name": "ember-nrg-ui",
  "version": "3.5.0",
  "description": "Opinionated UI addon based on how KUB scaffolds web applications",
  "keywords": [
    "ember-addon",
    "Fomantic UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/knoxville-utilities-board/ember-nrg-ui"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "build-dummy-app": "ember build --environment production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:js lint:hbs test:ember",
    "test:ember": "ember test",
    "test:cicd": "ember test -r xunit",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "dayjs": "1.10.7",
    "ember-auto-import": "^1.11.3",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-flash": "2.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-sass": "10.0.1",
    "ember-concurrency": "2.2.0",
    "ember-concurrency-decorators": "2.0.3",
    "ember-get-config": "0.5.0",
    "ember-is-mobile": "4.1.2",
    "ember-keyboard": "7.0.1",
    "ember-resize-observer-service": "^0.4.5",
    "ember-responsive": "4.0.2",
    "ember-tether": "^2.0.1",
    "ember-truth-helpers": "3.0.0",
    "ember-validators": "4.0.0",
    "fomantic-ui-css": "2.8.8",
    "git-repo-info": "^2.1.1",
    "markdown-it": "12.3.2",
    "sass": "1.43.5",
    "walk-sync": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "^3.0.0",
    "ember-data": "3.28.4",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-prism": "^0.10.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.6",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.64.4"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://knoxville-utilities-board.github.io/ember-nrg-ui/"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "clean-css": "^4.1.11",
    "glob-parent": "^5.1.2"
  }
}
